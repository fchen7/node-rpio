{
  "name": "node-rpio",
  "version": "0.9.22",
  "description": "High performance GPIO/i2c/PWM/SPI module for Raspberry Pi",
  "main": "./lib/rpio.js",
  "scripts": {
    "test": "true",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "dependencies": {
    "bindings": "*",
    "nan": "*",
	"node-pre-gyp": "latest"
  },
  "engines": {
    "node": ">=0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fchen7/node-rpio"
  },
  "keywords": [
    "bcm2835",
    "gpio",
    "gpiomem",
    "i2c",
    "mmap",
    "pi",
    "pwm",
    "raspberry",
    "raspberrypi",
    "raspberry pi",
    "rpi",
    "spi"
  ],
  "author": "Jonathan Perkin <jonathan@perkin.org.uk> (http://www.perkin.org.uk/)",
  "license": "(ISC AND GPL-2.0)",
  "gypfile": true,
   "devDependencies": {
    "aws-sdk": "latest"
  },
  "binary": {
    "module_name": "node-rpio",
    "module_path": "build/{configuration}/",
    "host": "https://node-rpio.s3-ap-northeast-1.amazonaws.com/"
  }
}
